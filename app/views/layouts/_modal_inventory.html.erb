<div class="modal-header">
                  <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                  <h3 id="myModalLabel">Inventario</h3>
</div>
                <table  class="table table-striped">
                   <thead>
                     <tr>
                       <th>Maestra</th>
                       <th>Descripción</th>
                     </tr>
                   </thead>

                   <tbody>
                    <% if can? :manage, Entrance %>
                      <tr>
                          <td><img src="assets/bodega.png", width='25'><%= link_to "Entradas", entrances_path %></td>
                          <td>Entradas</td>
                      </tr>
                    <% end %>
                    <% if can? :manage, ReceiptHeader %>
                      <tr>
                          <td><img src="assets/bodega.png", width='25'><%= link_to "Entradas", receipt_headers_path %></td><td>Almacen</td>
                      </tr>
                    <% end %>
                   </tbody>
                 </table>
                <div class="modal-footer">
                  <button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
                  
                </div>