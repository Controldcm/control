<tr id=<%= "procedure_#{procedure.id}" %>>

   <td>Proceso - <%= @procedures.index(procedure)+1 %>
       <%= image_tag procedure.condition.image.url(:small) %>
   </td>	
      
   <td>
       <%= link_to image_tag('ver.png', :width=>'30', :title=>'Mostrar' ), production_order_procedure_path(@production_order, procedure), :remote => true %>
       <%= link_to image_tag('edit.png', :width=>'30', :title=>'Editar' ),edit_production_order_procedure_path(@production_order, procedure), :remote => true %>
       <%= link_to image_tag('destroy.png', :width=>'30', :title=>'Eliminar' ), production_order_procedure_path(@production_order, procedure), :confirm => 'Are you sure you want to delete this entry? This is an irreversible action.', :method => :delete, :remote => true %>
   </td>
</tr>